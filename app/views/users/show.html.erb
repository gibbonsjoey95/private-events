<h1>User Profile: <%= @user.name %></h1>

<h2>Events Created by <%= @user.name %></h2>
<ul>
  <% @created_events.each do |event| %>
    <li>
      <strong><%= event.name %></strong><br>
      Date: <%= event.date.strftime('%B %d, %Y') %><br>
      Location: <%= event.location %>
    </li>
  <% end %>
</ul>